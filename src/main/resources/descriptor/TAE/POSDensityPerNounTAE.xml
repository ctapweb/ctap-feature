<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>  
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="SentenceAnnotator">
      <import name="descriptor.annotator.SentenceAnnotator"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="TokenAnnotator">
      <import name="descriptor.annotator.TokenAnnotator"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="POSAnnotator">
      <import name="descriptor.annotator.POSAnnotator"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="POSDensityPerNoun_ProperNouns_Feature">
      <import name="descriptor.featureAE.POSDensityPerNoun_ProperNouns_Feature"/>
    </delegateAnalysisEngine>
    </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>LexicalVariationTAE</name>
    <description/>
    <version>1.0</version>
    <vendor/>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>aeID</name>
        <type>Integer</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>POSDensityPerNoun_ProperNouns_Feature/aeID</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>POSType</name>
        <description>options:
	lexical		lexical pos: n, v, adj, and adv
	functional	non-lexical pos
	conjunction	CC, IN
	determiner	PDT, DT
	adjective		JJ, JJR, JJS
	noun		NN, NNS, NNP, NNPS
	pronoun		PRP, PRP$, WP, WP$
	adverb		RB, RBR, RBS, WRB
	verb			VB, VBD, VBG, VBN, VBP, VBZ
	CD
	EX
	FW
	LS
	MD
	POS
	RP
	SYM
	TO
	UH
	space separated multiple types are also allowed, e.g. "JJ NN WP"</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>POSDensityPerNoun_ProperNouns_Feature/POSType</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>aeID</name>
        <value>
          <integer>2222</integer>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>POSType</name>
        <value>
          <string>pronoun</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <flowConstraints>
      <fixedFlow>
        <node>SentenceAnnotator</node>
        <node>TokenAnnotator</node>
        <node>POSAnnotator</node>
        <node>POSDensityPerNoun_ProperNouns_Feature</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs/>
        <languagesSupported/>
      </capability>
    </capabilities>
  <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
