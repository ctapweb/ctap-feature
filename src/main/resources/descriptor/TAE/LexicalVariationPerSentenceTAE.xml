<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>  
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="SentenceAnnotator">
      <import name="descriptor.annotator.SentenceAnnotator"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="TokenAnnotator">
      <import name="descriptor.annotator.TokenAnnotator"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="POSAnnotator">
      <import name="descriptor.annotator.POSAnnotator"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="LexicalVariationPerSentence_Modifier_Feature">
      <import name="descriptor.featureAE.LexicalVariationPerSentence_Modifier_Feature"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>LexicalVariationTAE</name>
    <description/>
    <version>1.0</version>
    <vendor/>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>aeID</name>
        <type>Integer</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>LexicalVariationPerSentence_Modifier_Feature/aeID</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>LexicalType</name>
        <description>options:
	lexical		lexical pos: n, v, adj, and adv
	adjective		JJ, JJR, JJS
	noun		NN, NNS, NNP, NNPS
	adverb		RB, RBR, RBS, WRB
	verb			VB, VBD, VBG, VBN, VBP, VBZ
	modifier		adj + adv</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>LexicalVariationPerSentence_Modifier_Feature/LexicalType</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>aeID</name>
        <value>
          <integer>2222</integer>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>LexicalType</name>
        <value>
          <string>adverb</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <flowConstraints>
      <fixedFlow>
        <node>SentenceAnnotator</node>
        <node>TokenAnnotator</node>
        <node>POSAnnotator</node>
        <node>LexicalVariationPerSentence_Modifier_Feature</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs/>
        <languagesSupported/>
      </capability>
    </capabilities>
  <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
